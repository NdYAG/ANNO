{
  "name": "读书笔记",
  "description": "豆瓣读书笔记第三方应用",
  "version": "0.2.5",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "img/books.png", "128": "img/books.png" },
  "permissions": [
    {"fileSystem": ["write"]}
    , "storage"
    , "identity"
    , "https://www.douban.com/*", "https://api.douban.com/*", "http://*.douban.com/*"
    , "https://*.evernote.com/*", "http://*.evernote.com/*"
    , "https://*.yinxiang.com/*", "http://*.yinxiang.com/*"
    , "https://www.google-analytics.com/"
  ]
}
